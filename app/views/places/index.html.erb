<% @places.each do |place| %>
<h1><%= place.name %></h1>
<h3><%= place.description %></h3>
<h3><%= place.address %></h3>
<% end %>

<div id="map"
     id="geocoder"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-center="<%= @center.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>
